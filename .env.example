# This is an example environment file.
# Copy this file to .env and fill in the values.

# =============================================================================
# SUPERSET CONFIGURATION
# =============================================================================

# REQUIRED: Generate a new secret key with: openssl rand -base64 42
SUPERSET_SECRET_KEY=change_me

# Admin credentials (change in production!)
SUPERSET_ADMIN_USER=admin
SUPERSET_ADMIN_PASSWORD=admin
SUPERSET_ADMIN_EMAIL=admin@example.com

# =============================================================================
# DATABASE CONFIGURATION
# =============================================================================

# Business database (postgres-fsa)
POSTGRES_DB=FSA
POSTGRES_USER=postgres
POSTGRES_PASSWORD=change_me_in_production

# Superset metadata database (superset-db)
SUPERSET_DB_USER=superset
SUPERSET_DB_PASSWORD=change_me_in_production

# =============================================================================
# MIGRATION CONFIGURATION (optional)
# =============================================================================

# MariaDB source
MARIADB_HOST=localhost
MARIADB_USER=root
MARIADB_PASSWORD=
MARIADB_DB=

# PostgreSQL target
PG_HOST=localhost
PG_USER=postgres
PG_PASSWORD=
PG_DB=FSA
PG_SCHEMA=staging

# Performance
BATCH_SIZE=500
ENABLE_API_ENRICHMENT=false